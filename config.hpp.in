#cmakedefine FCPPT_HAVE_GCC_PRETTY_FUNCTION
#cmakedefine FCPPT_HAVE_GCC_VISIBILITY

// gnu
#cmakedefine FCPPT_HAVE_BACKTRACE

#endif
