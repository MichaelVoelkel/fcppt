#ifndef FCPPT_CONFIG_HPP_INCLUDED
#define FCPPT_CONFIG_HPP_INCLUDED

// general C++
#cmakedefine FCPPT_HAVE_ATLEAST_UINT64
#cmakedefine FCPPT_HAVE_LONG_LONG

// string config
#cmakedefine FCPPT_NARROW_STRING
#cmakedefine FCPPT_STRING_IS_UTF8

// compile extensions
#cmakedefine FCPPT_ALIGN_TEMPLATE_PARAMETER

